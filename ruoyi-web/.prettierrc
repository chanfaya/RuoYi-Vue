{
    "printWidth": 150,
    "tabWidth": 4,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "quoteProps": "preserve",
    "jsxSingleQuote": false,
    "bracketSameLine": false,
    "trailingComma": "none",
    "bracketSpacing": true,
    "embeddedLanguageFormatting": "auto",
    "arrowParens": "always",
    "requirePragma": false,
    "insertPragma": false,
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "css",
    "vueIndentScriptAndStyle": false,
    "endOfLine": "auto"
}
